{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard"
  ],
  "rules": {
    "semicolon": [
      true,
      "always"
    ],
    "ter-indent": [
      false
    ],
    "indent": [
      true,
      "tabs"
    ],
    "no-trailing-whitespace": false,
    "no-unused-expression": false
  }
}